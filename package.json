{
	"name": "twitter-teste",
	"version": "0.0.1",
	"type": "commonjs",
	"license": "MIT",
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.15",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/node": "~18",
		"@types/swagger-jsdoc": "^6.0.1",
		"@types/swagger-ui-express": "^4.1.3",
		"@typescript-eslint/eslint-plugin": "~5.44",
		"@typescript-eslint/parser": "~5.44",
		"eslint": "~8.28",
		"prisma": "^4.9.0",
		"ts-node-dev": "^2.0.0",
		"typescript": "~4.9"
	},
	"scripts": {
		"dev": "tsnd src/server.ts",
		"lint": "eslint . --ext .ts",
		"prebuild": "yarn lint && yarn swagger",
		"build": "tsc -p tsconfig.json",
		"prestart": "yarn migrate",
		"start": "node build/src/server.js",
		"migrate": "prisma migrate dev && yarn prisma generate",
		"connect": "docker exec -it db psql -Uadmin -d twitter"
	},
	"dependencies": {
		"@prisma/client": "^4.9.0",
		"bcrypt": "^5.1.0",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"jsonwebtoken": "^9.0.0"
	}
}
